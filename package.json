{
  "name": "template-npm",
  "version": "0.0.0",
  "main": "dist/src/example.js",
  "files": [
    "dist/**/*",
    "/npm-shrinkwrap.json"
  ],
  "bin": {
    "seeder": "./bin/run"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "clean": "rm -rf dist",
    "prettier": "prettier \"src/**/*.{js,ts,tsx}\" --check",
    "format": "prettier \"src/**/*.{js,ts,tsx}\" --write",
    "prepare": "yarn run build",
    "bump-version": " auto-upgrade-npm-version",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint"
  },
  "devDependencies": {
    "@types/jest": "24.0.14",
    "@types/node": "12.0.8",
    "auto-upgrade-npm-version": "1.0.7",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "ts-jest": "24.0.2",
    "tslint": "5.17.0",
    "typescript": "3.5.1"
  },
  "description": "seeder is a cli and library to publish fake audit log events to WorkOS.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxchehab/template-npm.git"
  },
  "author": "maxchehab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxchehab/template-npm/issues"
  },
  "homepage": "https://github.com/maxchehab/template-npm#readme",
  "dependencies": {
    "tslib": "1.10.0"
  }
}
